<?xml version="1.0" encoding="UTF-8"?>
<Audio version="1.0" timeUnit="ms" interactionArea="#touchArea" gain="-10dB">
	<Chain portamento="50">

		<OscillatorNode type="sawtooth">
			<frequency follow="relX" map="0,100,36,72, MIDI" steps="0 2 4 5 7 9 11 12"></frequency>
		</OscillatorNode>

		<BiquadFilterNode type="bandpass">
			<frequency follow="relY" map="0,100,0,5000"></frequency>
		</BiquadFilterNode>


		<GainNode>
			<gain>
				<Envelope adsr="100,100,50,50" trig="pointer" max="1"></Envelope>
			</gain>
		</GainNode>

	</Chain>

</Audio>
